{
  "name": "deadlocks-project",
  "version": "1.0.0",
  "description": "Deadlocks magic",
  "author": "ComicSans",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "cross-env PORT=3002 ts-node -r tsconfig-paths/register src/main",
    "start:dev": "nodemon",
    "start:debug": "cross-env nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^5.7.4",
    "@nestjs/core": "^5.4.0",
    "@nestjs/mongoose": "^5.2.2",
    "@nestjs/swagger": "^3.1.0",
    "@types/mongoose": "^5.10.1",
    "cross-env": "^7.0.3",
    "fs": "0.0.1-security",
    "jsonpath": "^1.0.2",
    "mongodb": "^3.2.7",
    "mongoose": "^5.8.11",
    "odata-sequelize": "^1.0.11",
    "odata-v4-filter-parser": "0.0.6",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.0.7",
    "typescript": "^3.7.5",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@nestjs/testing": "^5.1.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.14.13",
    "@types/supertest": "^2.0.8",
    "@types/xml2json": "^0.11.3",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "install": "^0.13.0",
    "jest": "^23.5.0",
    "nodemon": "^1.19.1",
    "npm": "^6.14.9",
    "prettier": "^1.18.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "type": "module"
}
